[
{:category :clojure.data.csv.functions,
 :parametric
 [{:describe "read-csv",
   :params
   {:input ["nil" "true" "0" "1" "-1" "42" "3.14" "\"hello\""]},
   :template "(clojure.data.csv/read-csv %input)"}
  {:describe "read-csv-from",
   :params
   {:input ["nil" "true" "0"],
    :sep ["nil" "true" "0"],
    :quote ["nil" "true" "0"]},
   :template "(clojure.data.csv/read-csv-from %input %sep %quote)"}
  {:describe "write-csv",
   :params
   {:writer ["nil" "true" "0" "1" "-1"],
    :data ["nil" "true" "0" "1" "-1"]},
   :template "(clojure.data.csv/write-csv %writer %data)"}]}
]
